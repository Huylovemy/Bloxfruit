-- ===== Copy Skill Tab =====
local CopyTab = Window:MakeTab({Name = "Copy Skill", PremiumOnly = false})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local copyTarget = nil
local copyItem = nil
local playerDropdown = nil

-- Danh sĂ¡ch item chi tiáº¿t cho tá»«ng category
local categoryItems = {
    Fruit = {"Angel","Evil","Destroy","Element"},
    Melee = {"Gojo","Sukuna","DragonCombatV2","Kaiser","Garp","Saitama","Garou","Vegeta","Itachi","Goku","Broly"},
    Sword = {"Guts","Cid","Geto","Escanor","Meta bat","Pickleball","Rayleigh","SungJinWoo","SiuStyle","Shinji","Gryphon"},
    Armor = {"Cursed Armor","Armor Epic"}
}

-- HĂ m refresh dropdown Player
local function refreshPlayerDropdown()
    local t = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then table.insert(t, plr.Name) end
    end
    if playerDropdown then
        playerDropdown:Refresh(t, true)
    end
end

-- Dropdown chá»n Player
playerDropdown = CopyTab:AddDropdown({
    Name = "Chá»n Player",
    Default = "None",
    Options = {},
    Callback = function(value)
        copyTarget = Players:FindFirstChild(value)
    end
})
refreshPlayerDropdown()
Players.PlayerAdded:Connect(refreshPlayerDropdown)
Players.PlayerRemoving:Connect(refreshPlayerDropdown)

-- Dropdown chá»n Fruit
CopyTab:AddDropdown({
    Name = "Chá»n Fruit",
    Default = categoryItems.Fruit[1],
    Options = categoryItems.Fruit,
    Callback = function(v)
        copyItem = v
    end
})

-- Dropdown chá»n Melee
CopyTab:AddDropdown({
    Name = "Chá»n Melee",
    Default = categoryItems.Melee[1],
    Options = categoryItems.Melee,
    Callback = function(v)
        copyItem = v
    end
})

-- Dropdown chá»n Sword
CopyTab:AddDropdown({
    Name = "Chá»n Sword",
    Default = categoryItems.Sword[1],
    Options = categoryItems.Sword,
    Callback = function(v)
        copyItem = v
    end
})

-- Dropdown chá»n Armor
CopyTab:AddDropdown({
    Name = "Chá»n Armor",
    Default = categoryItems.Armor[1],
    Options = categoryItems.Armor,
    Callback = function(v)
        copyItem = v
    end
})

-- HĂ m tĂ¬m skill remote
local function findSkillRemote(player, item, key)
    if not player then return nil end
    local containers = {player.Character, player:FindFirstChildOfClass("Backpack")}
    for _, container in ipairs(containers) do
        if container then
            for _, tool in ipairs(container:GetChildren()) do
                if tool:IsA("Tool") and string.find(tool.Name, item) then
                    local skillFolder = tool:FindFirstChild(key)
                    if skillFolder then
                        for _, ev in ipairs(skillFolder:GetChildren()) do
                            if ev:IsA("RemoteEvent") then
                                return ev
                            end
                        end
                    end
                end
            end
        end
    end
    return nil
end

-- NĂºt skill Z-X-C-V-F-N-K
for _, key in ipairs({"Z","X","C","V","F","N","K"}) do
    CopyTab:AddButton({
        Name = "DĂ¹ng Skill "..key,
        Callback = function()
            if copyTarget and copyItem then
                local remote = findSkillRemote(copyTarget, copyItem, key)
                if remote then
                    pcall(function() remote:FireServer() end)
                    print("ÄĂ£ copy skill "..key.." tá»« "..copyTarget.Name.." ("..copyItem..")")
                else
                    warn("KhĂ´ng tĂ¬m tháº¥y skill "..key.." tá»« "..copyTarget.Name.." ("..copyItem..")")
                end
            else
                warn("ChÆ°a chá»n player hoáº·c item!")
            end
        end
    })
end

-- ===== Auto Mua Äá»“ Tab =====
local CollectTab = Window:MakeTab({Name = "Auto Mua Äá»“", PremiumOnly = false})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local autoCollectFiltered = false
CollectTab:AddToggle({
    Name = "Auto Nháº·t Fruit/Melee/Item/Armor/Sword",
    Default = false,
    Callback = function(state)
        autoCollectFiltered = state
        task.spawn(function()
            while autoCollectFiltered do
                task.wait(0.3)
                for _, v in ipairs(workspace:GetDescendants()) do
                    if v:IsA("BasePart") then
                        local name = v.Name:lower()
                        local parentName = v.Parent and v.Parent.Name:lower() or ""

                        -- Lá»c Ä‘Ăºng fruit / melee / item / armor / sword
                        if string.find(name, "fruit") or string.find(parentName, "fruit")
                        or string.find(name, "melee") or string.find(parentName, "melee")
                        or string.find(name, "item") or string.find(parentName, "item")
                        or string.find(name, "armor") or string.find(parentName, "armor")
                        or string.find(name, "sword") or string.find(parentName, "sword") then
                            
                            -- Check TouchInterest
                            for _, child in ipairs(v:GetChildren()) do
                                if child:IsA("TouchTransmitter") then
                                    pcall(function()
                                        firetouchinterest(LocalPlayer.Character.HumanoidRootPart, v, 0)
                                        task.wait()
                                        firetouchinterest(LocalPlayer.Character.HumanoidRootPart, v, 1)
                                    end)
                                end
                                if child:IsA("ClickDetector") then
                                    pcall(function()
                                        fireclickdetector(child)
                                    end)
                                end
                                if child:IsA("ProximityPrompt") then
                                    pcall(function()
                                        fireproximityprompt(child)
                                    end)
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
})

-- ===== Check Backpack Tab =====
local BackpackTab = Window:MakeTab({Name = "Check Backpack", PremiumOnly = false})

local targetPlayer = nil

-- Dropdown chá»n player
local playerDropdown = BackpackTab:AddDropdown({
    Name = "Chá»n Player",
    Default = "None",
    Options = (function()
        local t = {}
        for _, plr in ipairs(Players:GetPlayers()) do
            table.insert(t, plr.Name)
        end
        return t
    end)(),
    Callback = function(value)
        targetPlayer = Players:FindFirstChild(value)
        if targetPlayer then
            OrionLib:MakeNotification({
                Name = "Check Backpack",
                Content = "Äang chá»n: " .. targetPlayer.Name,
                Time = 3
            })
        end
    end
})

-- NĂºt check backpack
BackpackTab:AddButton({
    Name = "Check Backpack",
    Callback = function()
        if not targetPlayer then
            OrionLib:MakeNotification({
                Name = "Check Backpack",
                Content = "ChÆ°a chá»n player!",
                Time = 3
            })
            return
        end

        local backpack = targetPlayer:FindFirstChild("Backpack")
        if not backpack then
            OrionLib:MakeNotification({
                Name = "Check Backpack",
                Content = targetPlayer.Name .. " khĂ´ng cĂ³ Backpack!",
                Time = 3
            })
            return
        end

        local items = {}
        for _, tool in ipairs(backpack:GetChildren()) do
            if tool:IsA("Tool") then
                table.insert(items, tool.Name)
            end
        end

        if #items == 0 then
            OrionLib:MakeNotification({
                Name = "Check Backpack",
                Content = targetPlayer.Name .. " backpack trá»‘ng!",
                Time = 4
            })
        else
            OrionLib:MakeNotification({
                Name = "Check Backpack",
                Content = targetPlayer.Name .. " cĂ³: " .. table.concat(items, ", "),
                Time = 8
            })
        end
    end
})

-- NĂºt refresh danh sĂ¡ch player
BackpackTab:AddButton({
    Name = "Refresh Player List",
    Callback = function()
        local t = {}
        for _, plr in ipairs(Players:GetPlayers()) do
            table.insert(t, plr.Name)
        end
        playerDropdown:Refresh(t, true)
    end
})

