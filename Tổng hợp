local HitboxTab = Window:MakeTab({Name="Hitbox Player", PremiumOnly=false})

local hitboxTarget = nil
local hitboxEnabled = false
local hitboxAllEnabled = false
local hitboxSizeValue = 50 -- máº·c Ä‘á»‹nh 50

-- Dropdown chá»n player
local hitboxDropdown = HitboxTab:AddDropdown({
    Name = "Select Player",
    Default = "None",
    Options = (function()
        local t = {}
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer then table.insert(t, plr.Name) end
        end
        return t
    end)(),
    Callback = function(value)
        hitboxTarget = Players:FindFirstChild(value)
    end
})

-- Slider chá»‰nh size hitbox
HitboxTab:AddSlider({
    Name = "Hitbox Size",
    Min = 10,
    Max = 10000, -- tá»‘i Ä‘a 10k
    Default = 50,
    Color = Color3.fromRGB(0,255,0),
    Increment = 10,
    ValueName = "Size",
    Callback = function(value)
        hitboxSizeValue = value
    end
})

-- Toggle báº­t táº¯t hitbox cho player chá»n
HitboxTab:AddToggle({
    Name = "Enable Hitbox (Selected)",
    Default = false,
    Callback = function(state)
        hitboxEnabled = state
        task.spawn(function()
            while hitboxEnabled do
                task.wait(0.2)
                if hitboxTarget and hitboxTarget.Character then
                    local hrp = hitboxTarget.Character:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        safe(function()
                            hrp.Size = Vector3.new(hitboxSizeValue, hitboxSizeValue, hitboxSizeValue)
                            hrp.Transparency = 0.7
                            hrp.BrickColor = BrickColor.new("Lime green")
                            hrp.Material = Enum.Material.Neon
                            hrp.CanCollide = false
                        end)
                    end
                end
            end
        end)
    end
})

-- Toggle báº­t táº¯t hitbox cho táº¥t cáº£ player
HitboxTab:AddToggle({
    Name = "Enable Hitbox (All Players)",
    Default = false,
    Callback = function(state)
        hitboxAllEnabled = state
        task.spawn(function()
            while hitboxAllEnabled do
                task.wait(0.2)
                for _, plr in ipairs(Players:GetPlayers()) do
                    if plr ~= LocalPlayer and plr.Character then
                        local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
                        if hrp then
                            safe(function()
                                hrp.Size = Vector3.new(hitboxSizeValue, hitboxSizeValue, hitboxSizeValue)
                                hrp.Transparency = 0.7
                                hrp.BrickColor = BrickColor.new("Lime green")
                                hrp.Material = Enum.Material.Neon
                                hrp.CanCollide = false
                            end)
                        end
                    end
                end
            end
        end)
    end
})

-- NĂºt Reset Hitbox (chá»‰ player chá»n)
HitboxTab:AddButton({
    Name = "Reset Hitbox (Selected)",
    Callback = function()
        if hitboxTarget and hitboxTarget.Character then
            local hrp = hitboxTarget.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                safe(function()
                    hrp.Size = Vector3.new(2,2,1) -- máº·c Ä‘á»‹nh
                    hrp.Transparency = 1
                    hrp.Material = Enum.Material.Plastic
                    hrp.BrickColor = BrickColor.new("Medium stone grey")
                    hrp.CanCollide = true
                end)
            end
        end
    end
})

-- NĂºt Reset Hitbox (táº¥t cáº£ player)
HitboxTab:AddButton({
    Name = "Reset Hitbox (All Players)",
    Callback = function()
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character then
                local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    safe(function()
                        hrp.Size = Vector3.new(2,2,1)
                        hrp.Transparency = 1
                        hrp.Material = Enum.Material.Plastic
                        hrp.BrickColor = BrickColor.new("Medium stone grey")
                        hrp.CanCollide = true
                    end)
                end
            end
        end
    end
})

