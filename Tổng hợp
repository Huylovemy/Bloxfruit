-- Biáº¿n lÆ°u player má»¥c tiĂªu
local tpTarget = nil

-- HĂ m láº¥y danh sĂ¡ch player trá»« báº£n thĂ¢n
local function getPlayerList()
    local list = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            table.insert(list, plr.Name)
        end
    end
    return list
end

-- Dropdown chá»n player
local tpDropdown = MiscTab:AddDropdown({
    Name = "Chá»n Player",
    Default = "None",
    Options = getPlayerList(),
    Callback = function(value)
        tpTarget = Players:FindFirstChild(value)
    end
})

-- NĂºt Teleport tá»›i player Ä‘Ă£ chá»n
MiscTab:AddButton({
    Name = "Teleport Tá»›i Player ÄĂ£ Chá»n",
    Callback = function()
        if tpTarget and tpTarget.Character and tpTarget.Character:FindFirstChild("HumanoidRootPart") then
            local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if myHRP then
                myHRP.CFrame = tpTarget.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
            end
        end
    end
})

-- Auto Teleport theo player Ä‘Ă£ chá»n
local autoTP = false
MiscTab:AddToggle({
    Name = "Auto Teleport Theo Player",
    Default = false,
    Callback = function(state)
        autoTP = state
        task.spawn(function()
            while autoTP do
                task.wait(0.2)
                if tpTarget and tpTarget.Character and tpTarget.Character:FindFirstChild("HumanoidRootPart") then
                    local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if myHRP then
                        pcall(function()
                            myHRP.CFrame = tpTarget.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
                        end)
                    end
                end
            end
        end)
    end
})

-- Cáº­p nháº­t dropdown khi player join/leave
Players.PlayerAdded:Connect(function(plr)
    table.insert(tpDropdown.Options, plr.Name)
    tpDropdown:Refresh(tpDropdown.Options)
end)
Players.PlayerRemoving:Connect(function(plr)
    for i, name in ipairs(tpDropdown.Options) do
        if name == plr.Name then
            table.remove(tpDropdown.Options, i)
            break
        end
    end
    tpDropdown:Refresh(tpDropdown.Options)
end)

-- ===== ThĂ´ng Tin YouTube =====
local ytLink = "https://youtube.com/@gauyeuvnhehe?si=Yal72RmtmdQb8cim"
MiscTab:AddLabel("YouTube: gauyeuvnhehe")

MiscTab:AddButton({
    Name = "Copy Link YouTube",
    Callback = function()
        if setclipboard then
            setclipboard(ytLink)
        else
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Copy Link",
                Text = "Clipboard khĂ´ng há»— trá»£, link: " .. ytLink,
                Duration = 3
            })
        end
    end
})

