-- ===== Toggle UI Button =====
local Player = game.Players.LocalPlayer

-- Táº¡o ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ToggleUI"
ScreenGui.Parent = Player:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Táº¡o ImageButton toggle
local ToggleButton = Instance.new("ImageButton")
ToggleButton.Name = "Toggle"
ToggleButton.Parent = ScreenGui
ToggleButton.Size = UDim2.new(0, 50, 0, 50)
ToggleButton.Position = UDim2.new(0, 10, 1, -10) -- gĂ³c dÆ°á»›i trĂ¡i
ToggleButton.AnchorPoint = Vector2.new(0, 1)
ToggleButton.Image = "rbxassetid://129218883294022"

-- Gradient xanh lĂ¡ -> xanh biá»ƒn
ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 255, 128)
ToggleButton.BackgroundTransparency = 0
local Gradient = Instance.new("UIGradient")
Gradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 255, 128)), -- xanh lĂ¡
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 128, 255))  -- xanh biá»ƒn
})
Gradient.Rotation = 45
Gradient.Parent = ToggleButton

-- Thiáº¿t láº­p hĂ¬nh dáº¡ng vĂ  kĂ©o tháº£
ToggleButton.ScaleType = Enum.ScaleType.Fit
ToggleButton.Draggable = true
local Corner = Instance.new("UICorner")
Corner.CornerRadius = UDim.new(0.2,0)
Corner.Parent = ToggleButton

-- Toggle UI báº±ng OrionLib khi click
ToggleButton.MouseButton1Click:Connect(function()
    if OrionLib then
        OrionLib:ToggleUi()
    end
end)
