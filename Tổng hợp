local SettingTab = Window:MakeTab({Name="Setting", PremiumOnly=false})

-- â¡ WalkSpeed
local walkspeed = 16
SettingTab:AddSlider({
    Name = "WalkSpeed",
    Min = 16,
    Max = 1000,
    Default = 16,
    Color = Color3.fromRGB(0,255,0),
    Increment = 1,
    ValueName = "Speed",
    Callback = function(value)
        walkspeed = value
        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then humanoid.WalkSpeed = walkspeed end
    end
})
-- Auto cáº­p nháº­t speed khi respawn
LocalPlayer.CharacterAdded:Connect(function(char)
    task.wait(1)
    local humanoid = char:FindFirstChildOfClass("Humanoid")
    if humanoid then humanoid.WalkSpeed = walkspeed end
end)

-- đŸ€ JumpPower
local jumppower = 50
SettingTab:AddSlider({
    Name = "JumpPower",
    Min = 50,
    Max = 1000,
    Default = 50,
    Color = Color3.fromRGB(255,0,0),
    Increment = 1,
    ValueName = "Jump",
    Callback = function(value)
        jumppower = value
        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then humanoid.JumpPower = jumppower end
    end
})
-- Auto cáº­p nháº­t jump khi respawn
LocalPlayer.CharacterAdded:Connect(function(char)
    task.wait(1)
    local humanoid = char:FindFirstChildOfClass("Humanoid")
    if humanoid then humanoid.JumpPower = jumppower end
end)

-- đŸ€ Infinite Jump
local infJump = false
SettingTab:AddToggle({
    Name = "Infinite Jump",
    Default = false,
    Callback = function(state)
        infJump = state
    end
})
game:GetService("UserInputService").JumpRequest:Connect(function()
    if infJump and LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
        LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end
end)

-- đŸŒ€ Noclip
local noclip = false
SettingTab:AddToggle({
    Name = "Noclip",
    Default = false,
    Callback = function(state)
        noclip = state
        task.spawn(function()
            while noclip do
                task.wait()
                local char = LocalPlayer.Character
                if char then
                    for _, v in ipairs(char:GetDescendants()) do
                        if v:IsA("BasePart") and v.CanCollide then
                            v.CanCollide = false
                        end
                    end
                end
            end
        end)
    end
})

