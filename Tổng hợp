Players.PlayerAdded:Connect(function(plr)
    hitboxDropdown:AddOption(plr.Name)
end)
Players.PlayerRemoving:Connect(function(plr)
    if plr.Name == (hitboxTarget and hitboxTarget.Name) then
        hitboxTarget = nil
    end
end)

