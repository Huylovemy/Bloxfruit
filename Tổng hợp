-- ===== ESP Tab NĂ¢ng Cao: Hiá»ƒn thá»‹ Sword + Devil Fruit + Melee + Item =====
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

local EspTab = Window:MakeTab({Name="ESP", PremiumOnly=false})
local espEnabled = false
local espLabels = {}

-- Láº¥y tĂªn tool / sword / fruit / melee player Ä‘ang dĂ¹ng
local function getPlayerItems(plr)
    local items = {}
    local char = plr.Character
    if not char then return "" end

    -- Tool / Sword Ä‘ang cáº§m
    local tool = char:FindFirstChildOfClass("Tool")
    if tool then table.insert(items, tool.Name) end

    -- Armor máº·c trĂªn ngÆ°á»i
    for _, armor in ipairs({
        "Cursed-Armor","Unique-Armor","Darkness-Armor","Thunder-Armor",
        "Diamond-Armor","Golden-Armor","Epic-Armor","Iron-Armor","Wooden-Armor"
    }) do
        if char:FindFirstChild(armor) then
            table.insert(items, armor)
        end
    end

    -- Láº¥y toĂ n bá»™ Tool trong Backpack (Devil Fruit / Sword / Item)
    if plr:FindFirstChild("Backpack") then
        for _, v in ipairs(plr.Backpack:GetChildren()) do
            if v:IsA("Tool") and not table.find(items, v.Name) then
                table.insert(items, v.Name)
            end
        end
    end

    -- Láº¥y thĂªm Tool trong Character (Melee, Sword, Fruit, Item khĂ¡c)
    for _, v in ipairs(char:GetChildren()) do
        if v:IsA("Tool") and not table.find(items, v.Name) then
            table.insert(items, v.Name)
        end
    end

    return table.concat(items, ", ")
end

-- Táº¡o hoáº·c cáº­p nháº­t ESP
local function updateESP(plr)
    if plr == LocalPlayer then return end
    local char = plr.Character
    if not char then return end
    local head = char:FindFirstChild("Head")
    if not head then return end

    local itemsText = getPlayerItems(plr)

    if espLabels[plr] then
        local label = espLabels[plr]:FindFirstChildOfClass("TextLabel")
        if label then
            label.Text = plr.Name .. "\n" .. itemsText
        end
    else
        local billboard = Instance.new("BillboardGui")
        billboard.Size = UDim2.new(0,200,0,100)
        billboard.Adornee = head
        billboard.AlwaysOnTop = true
        billboard.Name = "ESPLabel"

        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1,0,1,0)
        label.BackgroundTransparency = 1
        label.TextColor3 = Color3.new(1,0,0)
        label.TextStrokeTransparency = 0
        label.Font = Enum.Font.SourceSansBold
        label.TextScaled = true
        label.Text = plr.Name .. "\n" .. itemsText
        label.Parent = billboard

        billboard.Parent = PlayerGui
        espLabels[plr] = billboard
    end
end

local function removeESP(plr)
    if espLabels[plr] then
        espLabels[plr]:Destroy()
        espLabels[plr] = nil
    end
end

-- Toggle ESP
EspTab:AddToggle({
    Name = "Enable ESP (Player + Sword + Fruit + Item)",
    Default = false,
    Callback = function(state)
        espEnabled = state
        if espEnabled then
            for _, plr in ipairs(Players:GetPlayers()) do
                updateESP(plr)
            end
        else
            for plr, _ in pairs(espLabels) do
                removeESP(plr)
            end
        end
    end
})

-- Cáº­p nháº­t khi player join hoáº·c respawn
Players.PlayerAdded:Connect(function(plr)
    plr.CharacterAdded:Connect(function()
        if espEnabled then
            task.wait(1)
            updateESP(plr)
        end
    end)
end)

Players.PlayerRemoving:Connect(function(plr)
    removeESP(plr)
end)

-- VĂ²ng láº·p update liĂªn tá»¥c
task.spawn(function()
    while true do
        if espEnabled then
            for _, plr in ipairs(Players:GetPlayers()) do
                updateESP(plr)
            end
        end
        task.wait(0.5)
    end
end)

